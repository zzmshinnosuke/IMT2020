<Window x:Class="IMT2020.ParaNameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="设置保存参数名称" Height="71" Width="300" WindowStartupLocation="CenterScreen" >
    <Grid >
        <Label Content="参数名称:" HorizontalAlignment="Left" Margin="20,0,0,0" VerticalAlignment="Center"/>
        <TextBox x:Name="txt_name" HorizontalAlignment="Left" Height="23" Margin="80,0,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Center" Width="120"/>
        <Button x:Name="btn_ok" Content="确定" HorizontalAlignment="Left" Margin="207,0,0,0" VerticalAlignment="Center" Width="75" Click="btn_ok_Click"/>

    </Grid>
</Window>
